package tutor3;

import java.util.Scanner;

public class test22 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("Enter i : ");
		int i = scanner.nextInt();
		if(i <= 10) {
			System.out.println(i++);
			
		}
		else {
			System.out.println("Error");
		}
	}
}
