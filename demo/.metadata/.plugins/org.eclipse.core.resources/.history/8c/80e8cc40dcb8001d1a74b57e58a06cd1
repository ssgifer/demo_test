package tutor4;

import java.util.Scanner;

public class test141 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("Enter factorial : ");
		int f = scanner.nextInt(); 	
		int i, fact=1;  
//	 	for(i=1; i<=f; i++){    
//	 		fact = fact*i;    
//		}    
//	 	System.out.println("Factorial of "+ f +" is : " + fact); 
		while( i <= f ){  
            fact = fact * i;   
            i++; //increment i by 1   
        }     
  
        //printing the result  
        System.out.println("\nFactorial of " + f + " is: " + fact);  
}
}
