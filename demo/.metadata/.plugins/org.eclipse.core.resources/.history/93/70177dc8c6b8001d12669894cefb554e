package tutor3;

import java.util.Scanner;

public class test4 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("Enter N : ");
		int n = scanner.nextInt();
		if((n%2) == 0) {
			System.out.println("is odd number");
		}
		else {
			System.out.println("Error");
		}
	}
}
