package tutor4;

import java.util.Scanner;

public class ex_17 {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.print("Number : \n");
		int n = in.nextInt();
		for (int row=1; row<=n; row++) {
            for (int col=1; col<=row; col++) {
                System.out.print("*");
            }
            System.out.println();
        }
	}
}
