package demo;

import java.util.Scanner;

//import java.util.Scanner;

public class Shape {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		
		
		for (int i=n; i>=1; i--) {
			for (int j=1; i<=j; j++) {
	            System.out.print("*");
	        } 
	            System.out.println();
	    }
	}
}
